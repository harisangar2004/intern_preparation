vector<int> score(vector<int> team1, vector<int> team2) {
    
    sort(team1.begin(),team1.end());
    int count=0;
    int n1=team1.size() , n2=team2.size() ;
    vector<int> ans(n2);
    for(int i=0;i<n2;i++){
        for(int j=0;j<n1;j++){
            if(team1[j]<=team2[i]){
                ans[i]++;
            }
        }
    }
    return ans;
}
